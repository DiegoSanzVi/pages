<configuration debug="true" scan="true">      
 		
	 
	
   <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n\n</pattern>
		</encoder>
	</appender> 
	
	

 	<root level="INFO">
  <appender-ref ref="STDOUT"/> 
 		
		
 	</root>
 
    
</configuration>
